<Application x:Class="Floe.UI.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
			 xmlns:global="clr-namespace:Floe.UI">
    <Application.Resources>
		<global:App x:Key="App"/>
		<SolidColorBrush x:Key="winFG" Color="{Binding Source={x:Static global:App.Settings}, Path=Current.Colors.WindowForeground}"/>
	</Application.Resources>
</Application>
